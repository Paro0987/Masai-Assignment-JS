function substring(str, start, end) {
let result = "";
let from = Math.max(start, 0);
let to = Math.min(end);

if((end==null) || (end > str.length)) {
  for(let i = from; i<str.length; i++) {
  result+=str[i];
} 
}
else 
{ 
  for(let i = from; i<to; i++) {
   result+=str[i];
  }
}
return result;
}

let s =substring("paromita", 2, 4);
console.log(s);